.charactersOtherInput
  margin-top 10px
  padding-top 20px
  height 40px

  &_character
    display inline-block
    width 64px
    margin-right 5px
    text-align center
    border 2px solid #999
    box-shadow 1px 1px 3px #eee
    border-radius 5px

    &-selected
      border 2px solid blue

  &-selected
    background $select-color
